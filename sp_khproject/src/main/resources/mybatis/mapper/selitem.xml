<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="selitemMapper">
	<select id="getNextSeq" resultType="_int">
		SELECT SEL_ITEM_SEQ.NEXTVAL FROM DUAL
	</select>
 	<insert id="insertData" parameterType="selItemDto">
		INSERT INTO SEL_ITEM(SEL_TITLE, SEL_SELNAME, SEL_LOCATION, SEL_CONTENT)
			VALUES(#{sel_title}, #{sel_name}, #{sel_location}, #{sel_cintent})
	</insert>
	<select id=""></select>






</mapper>